import list

-- For desugaring list comprehensions ----------------------
mapList : (a -> b) -> List a -> List b
mapList _ [] = []
mapList f (a :: as) = f a :: mapList f as
------------------------------------------------------------

downfrom : N -> List N
downfrom 0     = []
downfrom (S n) = (n+1) :: downfrom n

even : N -> Bool
even 0     = true
even (S n) = not (even n)