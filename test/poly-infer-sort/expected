let f : forany a. (a → a) → a → a = g ↦ x ↦ g (g x) in f (x ↦ x + x) : ℕ → ℕ
let f : forany a. (a → a) → a → a = g ↦ x ↦ g (g x) in f (x ↦ x - x) : ℤ → ℤ
let f : forany a. (a → a) → a → a = g ↦ x ↦ g (g x) in f (x ↦ x / x) : 𝔽 → 𝔽
let f : forany a. (a → a) → a → a = g ↦ x ↦ g (g x) in f (x ↦ -x / x) : ℚ → ℚ
